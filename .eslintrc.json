{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["@nuxt/eslint-config", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "vue/multi-word-component-names": "off"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
